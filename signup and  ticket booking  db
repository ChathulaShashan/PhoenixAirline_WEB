create table tbooking( UID INT(10) ,TICKET_ID VARCHAR(10),FLIGHT_IDVARCHAR(10),GATEVARCHAR(10),SEATVARCHAR(10),dat int(10) )

create table flidgt ( flightid VARCHAR(10) , START VARCHAR(10), END VARCHAR(10), NO_OF_SEATS VARCHAR(10) ,PILOT_ID VARCHAR (10) )

create table SIGNUP ( FIRST_NAME VARCHAR(10) ,LAST_NAME VARCHAR(10), PASSWORD VARCHAR(10), E_MAIL VARCHAR(10), PHONE_N0 INT(20)  )
